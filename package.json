{
  "name": "webtoolbox",
  "version": "1.0.0",
  "description": "A collection of useful tools and resources for developers",
  "main": "index.html",
  "scripts": {
    "serve": "parcel index.html images/*",
    "build": "parcel build index.html images/* --public-url .",
    "updatepages": "git checkout master; cp CNAME dist/CNAME; git subtree split --prefix dist -b gh-pages; git push -f origin gh-pages:gh-pages; git branch -D gh-pages"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ToshNeox/webtoolbox.git"
  },
